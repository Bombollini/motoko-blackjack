{
  "version": 1,
  "projects": {
    "blackjack_backend": {
      "canister_id": "your_backend_canister_id",
      "type": "motoko",
      "source": [
        "src/blackjack_backend/main.mo"
      ]
    },
    "blackjack_frontend": {
      "canister_id": "your_frontend_canister_id",
      "type": "assets",
      "source": [
        "src/blackjack_frontend/src/index.html",
        "src/blackjack_frontend/src/index.js",
        "src/blackjack_frontend/src/index.css",
        "src/blackjack_frontend/assets"
      ]
    }
  },
  "defaults": {
    "build": {
      "packager": "dfx"
    }
  }
}